{
  "name": "lazy-query",
  "version": "2.2.0",
  "description": "Lazy evaluation querying library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "lazy",
    "query",
    "evaluation"
  ],
  "author": "Luka4ever",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.1",
    "jest": "^22.4.3",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.9.0",
    "typescript": "^2.8.1"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "pretest": "npm run build"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.test\\.ts$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|js)",
      "**/?(*.)test.(ts|js)"
    ]
  },
  "files": [
    "dist",
    "readme.md"
  ]
}
