{
	"compilerOptions": {
		"target": "es5",
		"module": "none",
		"lib": ["es6"],
		"declaration": true,
		"rootDir": "./src/",
		"importHelpers": true,
		"downlevelIteration": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"inlineSourceMap": true,
		"inlineSources": true,
		"forceConsistentCasingInFileNames": true
	}
}
